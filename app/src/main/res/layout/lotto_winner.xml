<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lotto_list_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/lotto_round"
        android:layout_width="69dp"
        android:layout_height="86dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:fontFamily="@font/nanana"
        android:gravity="center"
        android:text="@string/lotto_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lotto_win_1"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginStart="68dp"
        android:fontFamily="@font/nanana"
        android:gravity="left"
        android:text="TextView"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/lotto_win_2"
        app:layout_constraintStart_toEndOf="@+id/lotto_round"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread" />

    <TextView
        android:id="@+id/lotto_win_2"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:fontFamily="@font/nanana"
        android:gravity="left"
        android:text="TextView"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/lotto_win_3"
        app:layout_constraintStart_toStartOf="@+id/lotto_win_1"
        app:layout_constraintTop_toBottomOf="@+id/lotto_win_1" />

    <TextView
        android:id="@+id/lotto_win_3"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:fontFamily="@font/nanana"
        android:gravity="left"
        android:text="TextView"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/lotto_win_2"
        app:layout_constraintTop_toBottomOf="@+id/lotto_win_2" />

    <TextView
        android:id="@+id/prize_1"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginStart="184dp"
        android:fontFamily="@font/nanana"
        android:gravity="left"
        android:text="TextView"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/prize_2"
        app:layout_constraintStart_toEndOf="@+id/lotto_round"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread" />

    <TextView
        android:id="@+id/prize_2"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:fontFamily="@font/nanana"
        android:gravity="left"
        android:text="TextView"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/prize_3"
        app:layout_constraintStart_toStartOf="@+id/prize_1"
        app:layout_constraintTop_toBottomOf="@+id/prize_1" />

    <TextView
        android:id="@+id/prize_3"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:fontFamily="@font/nanana"
        android:gravity="left"
        android:text="TextView"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/prize_2"
        app:layout_constraintTop_toBottomOf="@+id/prize_2" />

</androidx.constraintlayout.widget.ConstraintLayout>