<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Returner_Myinfo_Modify_Result">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/apeach" />

    <TextView
        android:id="@+id/returner_myinfo_result_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="닉네임 : 반환자"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/returner_myinfo_result_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="아이디 : Jeong@naver.com"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/returner_myinfo_result_nickname"
        app:layout_constraintTop_toBottomOf="@+id/returner_myinfo_result_nickname" />

    <TextView
        android:id="@+id/returner_myinfo_result_tellnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="전화번호 : 010-0000-0000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/returner_myinfo_result_id"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/returner_myinfo_result_id"
        app:layout_constraintTop_toBottomOf="@+id/returner_myinfo_result_id" />

    <TextView
        android:id="@+id/returner_myinfo_result_loc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="내지역 : 서울"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/returner_myinfo_result_tellnum"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/returner_myinfo_result_tellnum"
        app:layout_constraintTop_toBottomOf="@+id/returner_myinfo_result_tellnum" />

    <Button
        android:id="@+id/returner_myinfo_result_btn_modify"
        android:layout_width="352dp"
        android:layout_height="39dp"
        android:text="수정하기"
        android:textColor="@color/black"
        app:backgroundTint="@color/purple_200"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>